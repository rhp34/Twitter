<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppBaseTheme" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="colorPrimary" tools:targetApi="l">@color/twitter_blue</item>
        <item name="titleTextColor" tools:targetApi="l">@color/cardview_light_background</item>
        <item name="icon" tools:targetApi="l">@drawable/twitter_logo_white</item>

    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/cardview_dark_background</item>

    </style>

    <style name="medium_green">#ff17bf63</style>
    <style name="medium_gray">#ffaab8c2</style>
    <style name="medium_gray_30">#4daab8c2</style>
    <style name="medium_gray_50">#80aab8c2</style>
    <style name="light_gray">#ffccd6dd</style>
    <style name="medium_red">#ffe0245e</style>
    <style name="medium_red_30">#4de0245e</style>
    <style name="medium_red_50">#80e0245e</style>
    <style name="twitter_blue">#ff1da1f2</style>
    <style name="twitter_blue_30">#4d1da1f2</style>
    <style name="twitter_blue_50">#801da1f2</style>
    <style name="twitter_blue_fill_pressed">#ff1370a9</style>

</resources>
